<?xml version="1.0" encoding="utf-8"?>
    <OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
        <Id>20aed075-f6ac-4d05-97cc-e97ecc59d2f7</Id>
        <Version>1.0</Version>
        <ProviderName>Microsoft</ProviderName>
        <DefaultLocale>en-us</DefaultLocale>
        <DisplayName DefaultValue="(local) Image callout add-in" />
        <Description DefaultValue="See how you can use the Canvas API with Word add-ins." />
        <!--Icon for your add-in. Used on installation screens and the add-ins dialog -->
        <IconUrl DefaultValue="https://127.0.0.1:8085/media/groupIcon1_default32.png" />
        <Hosts>
            <Host Name="Document" />
        </Hosts>
        <DefaultSettings>
            <SourceLocation DefaultValue="https://127.0.0.1:8085" />
        </DefaultSettings>
        <Permissions>ReadWriteDocument</Permissions>

        <!-- Add-in command declarations. See https://msdn.microsoft.com/EN-US/library/office/mt621545.aspx
        for more information about add-in commands. -->
        <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
            <Description resid="Group_ToolTip_Desc" />
            <Hosts>
                <Host xsi:type="Document">
                    <!-- DesktopFormFactor declares that the functionality is for Word on the Windows desktop. -->
                    <DesktopFormFactor>
                        <FunctionFile resid="InsertImageMenu_Url"/>

                    
                        <!-- PrimaryCommandSurface declares that this add-in 
                             command will be used from the ribbon in Office. -->
                        <ExtensionPoint xsi:type="PrimaryCommandSurface">
                            <!-- Add the add-in command button to the Developer tab. -->
                            <OfficeTab id="TabDeveloper">
                                <!-- This declares the group of buttons in the UI. -->
                                <Group id="Group1">
                                    <Label resid="GroupTitle" />
                                    <Icon>
                                        <bt:Image size="16" resid="groupIcon1_16x16" />
                                        <bt:Image size="32" resid="groupIcon1_32x32" />
                                        <bt:Image size="80" resid="groupIcon1_80x80" />
                                    </Icon>
                                    <Tooltip resid="Group_ToolTip_Desc" />
                                    <Control xsi:type="Button" id="Canvas.Button.Tab">

                                        <Label resid="Control_Label" />
                                        <Tooltip resid="Control_Tooltip" />
                                        <Supertip>
                                            <Title resid="Control_SuperTip_Label" />
                                            <Description resid="Control_SuperTip_Desc" />
                                        </Supertip>
                                        <Icon>
                                            <bt:Image size="16" resid="icon1_16x16" />
                                            <bt:Image size="32" resid="icon1_32x32" />
                                            <bt:Image size="80" resid="icon1_80x80" />
                                        </Icon>
                                        <Action xsi:type="ShowTaskpane">
                                            <SourceLocation resid="Sample_Url" />
                                        </Action>
                                    </Control>
                                </Group>
                            </OfficeTab>
                        </ExtensionPoint>
                        <!-- Declares a context menu that will open the add-in. -->
                        <ExtensionPoint xsi:type="ContextMenu">
                            <OfficeMenu id="ContextMenuText">
                                <Control xsi:type="Button" id="CanvasButtonMenu">

                                    <Label resid="Control_Label" />
                                    <Tooltip resid="Control_Tooltip" />
                                    <Supertip>
                                        <Title resid="Control_SuperTip_Label" />
                                        <Description resid="Control_SuperTip_Desc" />
                                    </Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="icon1_16x16" />
                                        <bt:Image size="32" resid="icon1_32x32" />
                                        <bt:Image size="80" resid="icon1_80x80" />
                                    </Icon>
                                    <Action xsi:type="ExecuteFunction">
                                        <FunctionName>contextMenuInsertImg</FunctionName>
                                    </Action>
                                </Control>
                            </OfficeMenu>
                        </ExtensionPoint>
                    </DesktopFormFactor>
                </Host>
            </Hosts>
            <!-- All attributes named resid refer to the resources 
                 found in the Resources element. -->
            <Resources>
                <bt:Images>
                    <!-- Image resource URLs. These must be located at HTTPS endpoints.-->
                    <bt:Image id="groupIcon1_16x16" DefaultValue="https://127.0.0.1:8085/media/groupIcon1_default16.png"></bt:Image>
                    <bt:Image id="groupIcon1_32x32" DefaultValue="https://127.0.0.1:8085/media/groupIcon1_default32.png"></bt:Image>
                    <bt:Image id="groupIcon1_80x80" DefaultValue="https://127.0.0.1:8085/media/groupIcon1_default80.png"></bt:Image>
                    <bt:Image id="icon1_16x16" DefaultValue="https://127.0.0.1:8085/media/icon_default16.png"></bt:Image>
                    <bt:Image id="icon1_32x32" DefaultValue="https://127.0.0.1:8085/media/icon_default32.png"></bt:Image>
                    <bt:Image id="icon1_80x80" DefaultValue="https://127.0.0.1:8085/media/icon_default80.png"></bt:Image>
                </bt:Images>
                <bt:Urls>
                    <bt:Url id="Sample_Url" DefaultValue="https://127.0.0.1:8085">
                        <bt:Override Locale="ja-jp" Value="https://127.0.0.1:8085" />
                    </bt:Url>
                    <bt:Url id="InsertImageMenu_Url" DefaultValue="https://127.0.0.1:8085/insertimage.html">
                        <bt:Override Locale="ja-jp" Value="https://127.0.0.1:8085/insertimage.html" />
                    </bt:Url>
                </bt:Urls>
                <!-- Be careful about mixing up what can be a short and a long string. 
                     A common issue with add-in commands is mixing short and long strings.
                     Read the documentation to make sure you use the correct string type.
                     https://msdn.microsoft.com/EN-US/library/office/mt621545.aspx -->
                <bt:ShortStrings>
                    <bt:String id="GroupTitle" DefaultValue="Canvas add-in group title">
                        <bt:Override Locale="ja-jp" Value="キャンバスアドイングループタイトル" />
                    </bt:String>
                    <bt:String id="Control_Label" DefaultValue="(local) Callout add-in">
                        <bt:Override Locale="ja-jp" Value="吹き出しは、アドイン" />
                    </bt:String>
                    <bt:String id="Control_Tooltip" DefaultValue="Canvas add-in3">
                        <bt:Override Locale="ja-jp" Value="キャンバスは、アドオン立方インチ" />
                    </bt:String>
                    <bt:String id="Control_SuperTip_Label" DefaultValue="Callout add-in supertip">
                        <bt:Override Locale="ja-jp" Value="吹き出しは、アドインsupertip" />
                    </bt:String>
                </bt:ShortStrings>
                <bt:LongStrings>
                    <bt:String id="Group_ToolTip_Desc" DefaultValue="(local) This add-in demonstrates how you can create an image editing add-in that uses the HTML Canvas API.">
                        <bt:Override Locale="ja-jp" Value="このアドインを使用すると、画像編集アドインそれをHTMLキャンバスAPIを使用して作成する方法を示します。" />
                    </bt:String>
                    <bt:String id="Control_SuperTip_Desc" DefaultValue="Open the canvas add-in in the task pane.">
                        <bt:Override Locale="ja-jp" Value="キャンバスアドイン作業ウィンドウを開きます。" />
                    </bt:String>
                </bt:LongStrings>
            </Resources>
        </VersionOverrides>
    </OfficeApp>